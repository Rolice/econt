{
  "name": "rolice/econt",
  "description": "Econt courier API integration in composer package",
  "type": "library",
  "version": "0.1",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "l.gardev@cloudcart.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-xml": "*",
    "ext-curl": "*",
    "nathanmac/parser": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "CloudCart\\Econt\\": "src",
      "CloudCart\\Econt\\Component\\": "src/Component/",
      "CloudCart\\Econt\\Exceptions\\": "src/exceptions/"
    }
  },
  "autoload-dev": {
    "psr-4": { "CloudCart\\Econt\\Test\\": "tests/" }
  }
}