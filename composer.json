{
  "name": "rolice/econt",
  "description": "Econt courier API integration in composer package",
  "type": "library",
  "version": "0.1",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "rolice@intellisys.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-xml": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "Rolice\\Econt\\": "src",
      "Rolice\\Econt\\Commands\\": "src/Commands/",
      "Rolice\\Econt\\Components\\": "src/Components/",
      "Rolice\\Econt\\Exceptions\\": "src/exceptions/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Rolice\\Econt\\Test\\": "tests/" }
  }
}